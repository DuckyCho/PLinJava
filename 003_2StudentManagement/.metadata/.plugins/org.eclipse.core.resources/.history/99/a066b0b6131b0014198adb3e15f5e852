package studentManagementSystem;

import java.util.HashMap;

public class Score {

	public static HashMap<String,Integer> subjectCode = new HashMap<String,Integer>();
	private int score;
	private String grade;
	
	
	public Score(int score){
		Score.subjectCode.put("math",1);
		Score.subjectCode.put("english",2);
		this.score = score;
	}
		
	public void setScore(int score){
		this.score = score;
	}
	
	public int getScore(){
		return this.score;
	}
	
	public void setGrade(String grade){
		this.grade = grade;
	}
	
	public String getGrade(){
		return this.grade;
	}
}
