package studentManagementSystem;

import java.util.HashMap;

public class Subjectcode {
	private static Subjectcode s1 = new Subjectcode();
	private static HashMap<String,Integer> subjectCode = new HashMap<String,Integer>();
	
	private Subjectcode(){
		Subjectcode.subjectCode.put("math",0);
		Subjectcode.subjectCode.put("english",1);
	}
	
	public int getSubjectcode(String subject){
		return Subjectcode.subjectCode.get(subject);
	}
}
