package game;

public class Player  {
	
	public String name;
	public GameLevel playerLevel;
	
	public Player(){
		this.name="Unnamed";
		this.playerLevel = GameLevel1.getInstance();}
	
	public Player(String name){
		this.name=name;
		this.playerLevel = GameLevel1.getInstance();}

	public void upgradeLevel(){
		
	}
	
	public void getGameLevel(){
		this.getLevel();
	}
	
	public void attack(){
		this.playerLevel.play();
	}
	
	
	
}
